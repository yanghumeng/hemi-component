{
  "private": false,
  "name": "hemi-component",
  "version": "1.0.2",
  "scripts": {
    "release": "cross-env UMI_ENV=prod father-build && lerna exec npx tsc-alias && lerna publish from-package",
    "release:beta": "git push --follow-tags && cross-env UMI_ENV=dev father-build && lerna exec npx tsc-alias && lerna publish from-package --dist-tag beta",
    "start": "dumi dev",
    "docs:build:test": "cross-env UMI_ENV=devDocs dumi build",
    "docs:build": "cross-env UMI_ENV=prod dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "build": "father build",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "lv:minor": "lerna version minor",
    "lv:minor:alpha": "lerna version preminor",
    "lv:minor:change-alpha": "lerna version preminor --conventional-prerelease",
    "lv:minor:change-stable": "lerna version minor --conventional-graduate",
    "lv:patch": "lerna version patch",
    "lv:patch:alpha": "lerna version prepatch",
    "lv:patch:change-alpha": "lerna version prepatch --conventional-prerelease",
    "lv:patch:change-stable": "lerna version patch --conventional-graduate",
    "lv:alpha": "lerna version prerelease",
    "lv:unknown": "git fetch origin master:master && git fetch origin test:test && node ./scripts/lerna-gitflow.js",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "src/[!.]*",
    "lib",
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.ts",
  "module": "src/index.ts",
  "typings": "dist/index.d.ts",
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "cz-customizable": {
    "config": ".cz-config.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "peerDependencies": {
    "@ant-design/icons": "^4.8.0",
    "antd": "^4.23.6",
    "moment": "^2.29.1",
    "react": "^16.12.0"
  },
  "dependencies": {
    "@ant-design/icons": "^4.8.0",
    "antd": "^4.23.6",
    "babel-plugin-import": "^1.13.5",
    "react": "^16.12.0"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.8.0",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-replace": "^3.0.1",
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^13.0.0",
    "@types/jest": "^27.0.3",
    "@types/lodash": "^4.14.189",
    "@umijs/fabric": "^2.8.1",
    "@umijs/test": "^3.0.5",
    "antd": "^4.23.6",
    "babel-plugin-import": "^1.13.5",
    "babel-plugin-module-resolver": "^4.1.0",
    "builtin-modules": "^3.2.0",
    "cross-env": "^7.0.3",
    "dumi": "^1.1.0",
    "father": "^4.0.0-rc.2",
    "gh-pages": "^3.0.0",
    "husky": "^8.0.2",
    "lerna": "^5.5.4",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.1",
    "rollup-plugin-inject-process-env": "^1.3.1",
    "react": "~16.12.0",
    "react-dom": "~16.12.0",
    "tsc-alias": "^1.6.0",
    "ttypescript": "^1.5.13",
    "typescript-transform-paths": "^3.3.1",
    "yorkie": "^2.0.0"
  },
  "workspaces": [
    "packages/*"
  ]
}
